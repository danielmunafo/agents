{
  "name": "agents",
  "version": "1.0.0",
  "description": "This repo summarizes tech trends in LinkedIn weekly through agents operations",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "pkgroll",
    "dev": "tsx src/index.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "fix": "npm run format && npm run lint:fix",
    "prepare": "husky install",
    "weekly": "tsx src/presentation/entrypoints/weekly.ts",
    "monthly": "tsx src/presentation/entrypoints/monthly.ts",
    "daily": "tsx src/presentation/entrypoints/daily.ts",
    "test:agent": "tsx src/presentation/entrypoints/test.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.0",
    "pkgroll": "^1.6.0",
    "prettier": "^3.2.4",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.1",
    "@types/puppeteer": "^5.4.7",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "openai": "^6.9.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "puppeteer": "^24.31.0",
    "zod": "^3.25.76"
  }
}
